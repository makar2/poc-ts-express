{
  "name": "sample-ts-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:prod": "rsync -r src/ dist/ --exclude='*.ts' && tsc && echo '>>> Done compiling' && node dist/bin/www.js",
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch src/ -e ts --exec npm run start:prod",
    "build2": "tsc"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.6.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
